{
  "name": "@shaed/webpack-config",
  "version": "0.0.1",
  "description": "Basic webpack config for typescript and react projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && babel src -d dist",
    "clean": "rimraf dist",
    "dev": "babel src -d dist --watch",
    "prepublishOnly": "yarn build",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisarevir/shaed.git"
  },
  "keywords": [
    "components",
    "javascript",
    "library",
    "react",
    "ts",
    "typescript",
    "webpack"
  ],
  "author": "chrisarevir",
  "license": "MIT",
  "bugs": "https://github.com/chrisarevir/shaed/issues",
  "dependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "mini-css-extract-plugin": "0.6.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "precss": "4.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "5.4.4",
    "typescript": "3.4.5",
    "webpack": "4.30.0",
    "webpack-merge": "4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "husky": "1.3.1",
    "jest": "25.1.0",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0"
  }
}
